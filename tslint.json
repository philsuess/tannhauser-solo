{
  "defaultSeverity": "warning",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "max-line-length": [true, 120],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "strict-boolean-expressions": false
  },
  "rulesDirectory": []
}
